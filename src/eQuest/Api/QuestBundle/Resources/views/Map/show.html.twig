{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Map</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Building</th>
                <td>{{ entity.building }}</td>
            </tr>
            <tr>
                <th>Floor_no</th>
                <td>{{ entity.floorno }}</td>
            </tr>
            <tr>
                <th>Img</th>
                <td>{{ entity.img }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_map_') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_map__edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
