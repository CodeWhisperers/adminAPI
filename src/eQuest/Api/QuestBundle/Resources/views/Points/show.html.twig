{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Points</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Details</th>
                <td>{{ entity.details }}</td>
            </tr>
            <tr>
                <th>X</th>
                <td>{{ entity.x }}</td>
            </tr>
            <tr>
                <th>Y</th>
                <td>{{ entity.y }}</td>
            </tr>
            <tr>
                <th>Map_id</th>
                <td>{{ entity.mapid }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Qr_code</th>
                <td>{{ entity.qrcode }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_points_') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_points__edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
